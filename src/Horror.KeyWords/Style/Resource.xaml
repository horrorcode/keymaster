  <FontFamily x:Key="FontBold">pack://application:,,,/Vestas.VestasOnline.Client.Common;component/Themes/Fonts/#Vestas Sans Bold</FontFamily>
  <FontFamily x:Key="FontBook">pack://application:,,,/Vestas.VestasOnline.Client.Common;component/Themes/Fonts/#Vestas Sans Book</FontFamily>
  <FontFamily x:Key="FontExtrabold">pack://application:,,,/Vestas.VestasOnline.Client.Common;component/Themes/Fonts/#Vestas Sans Extrabold</FontFamily>
  <FontFamily x:Key="FontHeadline">pack://application:,,,/Vestas.VestasOnline.Client.Common;component/Themes/Fonts/#Vestas Sans Headline</FontFamily>
